{
  "name": "vector-service",
  "version": "1.0.0",
  "description": "Production-ready vector search service with Qdrant backend for n8n workflows",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "vector",
    "search",
    "qdrant",
    "n8n",
    "embedding",
    "mongodb"
  ],
  "author": "Vector Service Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/static": "^6.12.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "googleapis": "^157.0.0",
    "mongodb": "^6.3.0",
    "multer": "^2.0.2",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
